<meta http-equiv="Content-Type" content="text/html; charset=utf-8">

<body>
  <center>
    <%= wicked_pdf_image_tag 'eLugarLogo.png', :width=>"500"%>
      <br><br><br>
      <h2>Relatório de Comparação</h2>
    <table>
      <br><br>
      <tr height="40">
        <td>
          <h2><%= @city1.name %></h2>
        </td>
        <td width="500">
        </td>
        <td>
          <h2><%= @city2.name %></h2>
        </td>
      </tr>
      <% @city1.attributes.each do |attr_name, attr_value| %>
          <% if (attr_name == 'id' || attr_name == 'name' || attr_name == 'image' || attr_name == 'created_at' || attr_name == 'updated_at' || attr_name == 'uber') %>
            <% next %>
          <% end %>
          <tr height="40">
            <td width="50">
              <%= attr_value%>
            </td>
            <td width="500">
              <center>
                <%= "#{@hash[attr_name]} #{@hashMetric[attr_name]}" %>
              </center>
            </td>
            <td>
              <%= @city2.send(attr_name.to_sym) %>
            </td>
          </tr>
      <%end%>

    </table>
  </center>
</body>
